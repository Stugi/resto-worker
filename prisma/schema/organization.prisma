model Organization {
  id   String @id
  name String

  billing     Billing?
  users       User[]
  restaurants Restaurant[]
  payments    Payment[]

  createdAt DateTime  @default(now())
  createdBy String?
  updatedAt DateTime  @updatedAt
  updatedBy String?
  deletedAt DateTime?
  deletedBy String?
}
